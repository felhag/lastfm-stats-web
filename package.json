{
  "name": "lastfm-stats-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "ngcc:setup": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "both": "npm run ngcc:setup && concurrently --kill-others \"ng serve --project=\"spotify-stats\" --port 4201\" \"ng serve --project=\"lastfm-stats\"",
    "build": "ng build",
    "lint": "ng lint",
    "deploy": "ng build --configuration production --project \"lastfm-stats\""
  },
  "private": true,
