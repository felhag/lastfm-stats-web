    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.1.4",
    "@angular/compiler-cli": "^15.1.4",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
